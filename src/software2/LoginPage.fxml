<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="software2.LoginPageController">
   <children>
      <VBox fx:id="bx" prefHeight="400.0" prefWidth="640.0">
        <children>
          <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                  <PasswordField fx:id="password" layoutX="251.0" layoutY="209.0" />
                  <TextField layoutX="251.0" layoutY="139.0" />
                  <Text layoutX="298.0" layoutY="135.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
                  <Text layoutX="300.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
                  <Button fx:id="submit" layoutX="298.0" layoutY="276.0" mnemonicParsing="false" text="Submit" />
                  <MenuButton fx:id="Language" layoutX="546.0" layoutY="14.0" mnemonicParsing="false" text="English">
                    <items>
                      <MenuItem fx:id="English" mnemonicParsing="false" text="English" />
                      <MenuItem fx:id="Spanish" mnemonicParsing="false" text="Spanish" />
                    </items>
                  </MenuButton>
            </children>
          </AnchorPane>
        </children>
      </VBox>
   </children>
</StackPane>
